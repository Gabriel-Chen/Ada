ocation) {      var node = document.querySelector('.js-onboarding__loc-manager .primary .main-card .location-name');      if (node && primaryLocation && primaryLocation.prsntNm) {        node.innerHTML = primaryLocation.prsntNm;      }      var links = document.querySelectorAll('.js-onboarding__loc-manager .primary .main-card .pre-link-a');      var divs = document.querySelectorAll('.js-onboarding__loc-manager .primary .main-card .pre-link-div');      if (links && links.length && primaryLocation && primaryLocation.fullLocId) {        function forEach(collection, iteratee) {  var func = isArray(collection) ? arrayEach : baseEach;  return func(collection, castFunction(iteratee));}(divs, function(divTag) {          divTag.style.display = 'none';        });        function forEach(collection, iteratee) {  var func = isArray(collection) ? arrayEach : baseEach;  return func(collection, castFunction(iteratee));}(links, function(link) {          var href = link.href;          link.href = href.replace('LOCID', primaryLocation.fullLocId);          link.style.display = 'block';        });      }    };    var setObservation = function(primaryObservation) {      var node = document.querySelector('.js-onboarding__loc-manager .primary .main-card .wx-temperature');      if (node && primaryObservation && primaryObservation.temperature) {        node.innerHTML = primaryObservation.temperature + '&deg;';      }      var nbnode = document.querySelector('.js-onboarding__loc-manager .primary .main-card .now-blurb');      if (nbnode && primaryObservation && primaryObservation.phrase) {        nbnode.innerHTML = primaryObservation.phrase;      }    };    var setHighLow = function(high, low) {      var hnode = document.querySelector('.js-onboarding__loc-manager .primary .main-card .high-temp');      var lnode = document.querySelector('.js-onboarding__loc-manager .primary .main-card .low-temp');      if (hnode && high) {        var hnodeHtml = hnode.innerHTML;        hnode.innerHTML = hnodeH